
% Parameters of Euler
t0 = 0;
y0 = 1;
T = 1;
h = 0.1;

function [res] = euler(T, h, funcion, y0)
    y = zeros(T/h-1);
    y(1) = y0;
    for i=1:length(y)
        y(i+1) = y(i) + h*funcion(h*i, y(i));
    end
    res = y;
end

